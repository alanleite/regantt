{
  "name": "regantt",
  "version": "0.0.1",
  "description": "A React Gantt Chart",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alanleite/regantt.git"
  },
  "keywords": [
    "react",
    "gantt",
    "chart"
  ],
  "author": "Alan Leite",
  "license": "MIT",
  "bugs": {
